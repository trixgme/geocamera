{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=(*), geolocation=(*)"
        }
      ]
    }
  ],
  "functions": {
    "api/time.ts": {
      "memory": 128,
      "maxDuration": 5
    }
  }
}
