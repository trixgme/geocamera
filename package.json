{
  "name": "geocamera",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && node dev-server.mjs",
    "dev:watch": "esbuild src/main.ts --bundle --outfile=public/js/app.js --sourcemap --watch",
    "build": "esbuild src/main.ts --bundle --outfile=public/js/app.js --minify --target=es2020",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "esbuild": "^0.21.0",
    "typescript": "^5.5.0",
    "@vercel/node": "^3.0.0"
  }
}
